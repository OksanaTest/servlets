<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Servlets Tutorial</display-name>

    <servlet>
        <servlet-name>FirstServlet</servlet-name>
        <servlet-class>FirstServlet</servlet-class>
        <init-param>
            <param-name>one</param-name>
            <param-value>1</param-value>
        </init-param>
        <init-param>
            <param-name>two</param-name>
            <param-value>1</param-value>
        </init-param>
        <init-param>
            <param-name>operation</param-name>
            <param-value>div</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>ServletfromWizard</servlet-name>
        <servlet-class>ServletfromWizard</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletfromWizard</servlet-name>
        <url-pattern>/ServletfromWizard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FirstServlet</servlet-name>
        <url-pattern>/FirstServlet</url-pattern>
    </servlet-mapping>
</web-app>